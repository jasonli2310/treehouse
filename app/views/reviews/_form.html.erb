<%= form_for([@property, @property.reviews.new]) do |f| %>

	<div id="user_stars"></div>
	<%= f.text_field :comment %>

	<%= f.hidden_field :property_id, value: @property.id %>

	<div class="actions">
		<%= f.submit %>
	</div>

<% end %>

	<script>
	$('#user_stars').raty({
		path: '/assets',
		scoreName: 'review[star]',
		score: 1
	});
	</script>
